{
    "manifest_version": 2,
  
    "name": "Sticky Notes",
    "description": "Take notes with sticky notes on any webpage",
    "version": "0.0.1",
    "browser_action": {
      "default_popup": "index.html",
      "default_title": "Open the popup"
    },
    "icons": {
	    "64": "blue_icon.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "css": [],
        "js": ["app-bundle.js"]
      }
    ],
    "permissions": [
    ]
  }
  
